                $("#dgCommentDataContent").disableSelection();

                $(".txtComment").click(function () {
                    var _positLeft = ($(window).width() - $("#dgCommentData").width()) / 2;
                    var _positTop = ($(window).height() - $("#dgCommentData").height()) / 2;

                    var _CommtCode = $(this).data("costcenter");
                    var _CommtMonthCode = $(this).data("month");
                    var _CommtYear = new Date().getFullYear();

