                    url: "BudgetCostComment.ashx",
                    data: {
                        pMode: "get",
                        pCmmtCode: _CmmtCode,
                        pYear: _CmmtYear,
                        pBy: "AUKIT.K"
                    },
                    contentType: "text/plain; charset=utf-8",
                    dataType: "text",
                    async: false,
                    beforeSend: function () {
                        $("#btnCommentSave, #btnCommentClose, .txtCmmt").prop('disabled', true);
                    },
                    success: function (response) {
                        var spData = response.split('|');

                        $(".txtComment").each(function () {
                            var CostCenterData = $(this).data("costcenter");
                            var MonthData = $(this).data("month");

                            if (CostCenterData == _CmmtCode) {
                                switch(MonthData){
                                    case "APR": $(this).val(spData[0]); break;
                                    case "MAY": $(this).val(spData[1]); break;
                                    case "JUN": $(this).val(spData[2]); break;
                                    case "JUL": $(this).val(spData[3]); break;
                                    case "AUG": $(this).val(spData[4]); break;
                                    case "SEP": $(this).val(spData[5]); break;
                                    case "OCT": $(this).val(spData[6]); break;
                                    case "NOV": $(this).val(spData[7]); break;
                                    case "DEC": $(this).val(spData[8]); break;
                                    case "JAN": $(this).val(spData[9]); break;
                                    case "FEB": $(this).val(spData[10]); break;
                                    case "MAR": $(this).val(spData[11]); break;
                                }
                            }
                        });

                        $("#btnCommentSave, #btnCommentClose, .txtCmmt").prop('disabled', false);
                    }
                });
            }


            function loadCommentFormMonthData(_CmmtCode, _CmmtYear, _CmmtMonth) {
                $.ajax({
                    async: true,
                    type: "GET",
                    url: "BudgetCostComment.ashx",
                    data: {
                        pMode: "get",
                        pCmmtCode: _CmmtCode,
                        pYear: _CmmtYear,
                        pBy: "AUKIT.K"
                    },
                    contentType: "text/plain; charset=utf-8",
                    dataType: "text",
                    async: false,
                    beforeSend: function () {
                        $("#btnCommentDataSave, #btnCommentDataClose, #txtCmmtData").prop('disabled', true);
                    },
                    success: function (response) {
                        var spData = response.split('|');
                        
                        switch (_CmmtMonth) {
                            case "APR": $("#txtCmmtData").val(spData[0]); break;
                            case "MAY": $("#txtCmmtData").val(spData[1]); break;
                            case "JUN": $("#txtCmmtData").val(spData[2]); break;
                            case "JUL": $("#txtCmmtData").val(spData[3]); break;
                            case "AUG": $("#txtCmmtData").val(spData[4]); break;
                            case "SEP": $("#txtCmmtData").val(spData[5]); break;
                            case "OCT": $("#txtCmmtData").val(spData[6]); break;
                            case "NOV": $("#txtCmmtData").val(spData[7]); break;
                            case "DEC": $("#txtCmmtData").val(spData[8]); break;
                            case "JAN": $("#txtCmmtData").val(spData[9]); break;
                            case "FEB": $("#txtCmmtData").val(spData[10]); break;
                            case "MAR": $("#txtCmmtData").val(spData[11]); break;
                            default: $("#txtCmmtData").val(""); break;
                        }

                        $("#btnCommentDataSave, #btnCommentDataClose, #txtCmmtData").prop('disabled', false);
                    }
                });
            }

        </script>
